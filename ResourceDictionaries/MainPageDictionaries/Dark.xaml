    <SolidColorBrush x:Key="Brush_Accent1Foreground" Color="White"/>
    <SolidColorBrush x:Key="Brush_Accent2Background" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="Brush_Accent2Foreground" Color="White"/>
